<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>
            loginPage.jsp
        </welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>logProcess</servlet-name>
        <servlet-class>logProcess</servlet-class>
        <!--<jsp-file>/loginPage.jsp</jsp-file>-->
    </servlet>
    <servlet-mapping>
        <servlet-name>logProcess</servlet-name>
        <url-pattern>/logexe</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>userProcessing</servlet-name>
        <servlet-class>userProcess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userProcessing</servlet-name>
        <url-pattern>/userexe</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>viewProcessing</servlet-name>
        <servlet-class>viewProcess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewProcessing</servlet-name>
        <url-pattern>/viewexe</url-pattern>
    </servlet-mapping>
</web-app>
